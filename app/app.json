{
  "expo": {
    "name": "SplitSahi",
    "slug": "splitsahi",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "splitsahi",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#020617"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.splitsahi.app",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "upi",
          "tez",
          "phonepe",
          "paytm",
          "bhim"
        ]
      }
    },
    "icon": "./assets/images/icon.png",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#020617"
      },
      "package": "com.piyushparadkar.splitsahise",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "upi",
              "host": "pay"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-asset"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f74e5159-7634-48f2-957e-67e65350af76"
      }
    }
  }
}
